<% current_user.trips.each do |trip| %>
    <p><%= trip.country %></p>
    <p><%= trip.city %></p>
    <p><%= trip.date_visited %></p>
    <p><%= trip.category %></p>
    <% trip.highlights.each do |highlight| %>
        <p><%= highlight.category %></p>
        <p><%= highlight.place %></p>
    <% end %>

<% end %>